{
	"name": "Django dev container",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "django",
	"workspaceFolder": "/app/",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode-remote.remote-containers",
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.black-formatter",
				"ms-azuretools.vscode-docker",
				"matangover.mypy",
				"antfu.unocss",
				"tamasfe.even-better-toml",
				"charliermarsh.ruff"
			]
		}
	}
}